<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="@dimen/y33"
    >

	<RelativeLayout
	    style="@style/boarder"
	    android:layout_width="wrap_content"
	    android:layout_height="@dimen/y133"
	    android:layout_alignParentBottom="true"
	    android:layout_alignParentLeft="true"
	    android:layout_alignParentRight="true"
	    android:layout_alignParentTop="true" >

	    <RelativeLayout
	        android:id="@+id/relativeLayout1"
	        android:layout_width="@dimen/x260"
	        android:layout_height="@dimen/y320"
	        android:layout_centerHorizontal="true"
	        android:layout_centerVertical="true"
	         style="@style/innerborder" >
	        <TextView 
				    android:id="@+id/CargpInformation" 
				   style="@style/textview_manywords"
				    android:text=" 无数据或数据出错"
				 />
	    </RelativeLayout>

	    <ImageButton
	        android:id="@+id/backImageButton"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_marginRight="@dimen/x5"
	        android:layout_alignTop="@+id/relativeLayout1"
	        android:background="@drawable/cancel" />

	</RelativeLayout>

</RelativeLayout>




